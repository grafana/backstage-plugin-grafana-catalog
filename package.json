{
  "name": "@grafana/catalog-backend-module-grafana-servicemodel",
  "description": "The grafana-service-model backend module for the catalog plugin.",
  "homepage": "https://github.com/grafana/catalog-backend-module-grafana-servicemodel",
  "version": "0.3.30",
  "main": "src/index.ts",
  "types": "src/index.ts",
  "author": "Grafana Labs",
  "license": "Apache-2.0",
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs.js",
    "types": "dist/index.d.ts"
  },
  "repository": {
    "type": "git",
    "url": "github.com/grafana/catalog-backend-module-grafana-servicemodel"
  },
  "keywords": [
    "backstage",
    "grafana",
    "plugin",
    "backend"
  ],
  "backstage": {
    "role": "backend-plugin-module",
    "pluginId": "catalog",
    "pluginPackage": "@backstage/plugin-catalog-backend"
  },
  "scripts": {
    "start": "backstage-cli package start",
    "build": "tsc --project tsconfig.json && backstage-cli package build",
    "lint": "backstage-cli package lint",
    "test": "backstage-cli package test",
    "clean": "backstage-cli package clean",
    "prepack": "backstage-cli package prepack",
    "postpack": "backstage-cli package postpack",
    "prepare": "husky && ./scripts/install-hooks.sh",
    "tsc": "tsc --project tsconfig.json",
    "install-hooks": "./scripts/install-hooks.sh"
  },
  "dependencies": {
    "@backstage/backend-plugin-api": "^1.6.0",
    "@backstage/backend-test-utils": "^1.0.2",
    "@backstage/catalog-model": "^1.7.0",
    "@backstage/config": "^1.2.0",
    "@backstage/errors": "^1.2.4",
    "@backstage/plugin-catalog-backend": "^3.3.0",
    "@backstage/plugin-catalog-common": "^1.0.25",
    "@backstage/plugin-catalog-node": "^1.12.4",
    "@kubernetes/client-node": "^1.1.2",
    "@spotify/prettier-config": "^15.0.0",
    "@types/lodash": "^4.14.202",
    "jsonpath-plus": "^10.2.0",
    "lodash": "^4.17.21",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@backstage/backend-test-utils": "^1.0.2",
    "@backstage/cli": "^0.35.1",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.202",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "lint-staged": "^16.0.0",
    "prettier": "^2.4.1",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist/**/*",
    "configSchema.d.ts",
    "README.md",
    "LICENSE"
  ],
  "configSchema": "configSchema.d.ts",
  "prettier": "@spotify/prettier-config",
  "resolutions": {
    "jsonpath-plus": "^10.3.0",
    "path-to-regexp": "^8.2.0",
    "tough-cookie": "^4.1.4",
    "ws": "^8.17.1",
    "micromatch": "^4.0.8",
    "undici": "^5.28.5",
    "koa": "2.16.3",
    "tar-fs": "^3.1.1",
    "@babel/runtime": "7.26.10",
    "@octokit/request": "9.2.1",
    "@octokit/request-error": "5.1.1",
    "@octokit/plugin-paginate-rest": "11.4.1",
    "http-proxy-middleware": "2.0.9",
    "sha.js": "^2.4.12",
    "node-forge": "^1.3.2",
    "jws": "^4.0.1",
    "qs": "^6.14.1",
    "form-data": "^4.0.4",
    "axios": "^1.12.0",
    "multer": "^2.0.2",
    "tar": "^7.5.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,mjs,cjs}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "packageManager": "yarn@1.22.22"
}
